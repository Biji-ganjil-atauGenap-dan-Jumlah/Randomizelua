"\124"..(foss[f^"\081\106"]or"\048"))))))))end end end else for Z,g in pairs(splicer)do splcIngfo=splcIngfo..("\n"..(g:upper()..("\124"..((waktu[g]or"\063")..("\124"..((statusWorld[g]or"\063")..("\124"..(foss[g]or"\048"))))))))end end end function taking()hookInfo("\084\097\107\105\110\103")sleep(100)if modeStorage=="\115\097\109\101"then for g,Y in pairs(str1)do warping(Y,worldFirstStorageId)sleep(100)local f=Z({Qr=scanFloatFirst()<=0 or scanFloatSecond()<=0},{__index=function(Z,g)Z.Qr=g end,__add=function(Z,g)return rawget(Z,"\081\114")end})if scanFloatFirst()>0 and scanFloatSecond()>0 then for Z,g in pairs(world:getObjects())do if g.id==firstSeedId then while not bot:isInTile(math.floor(g.x/32),math.floor(g.y/32))and inventory:getItemCount(firstSeedId)<=0 do bot:findPath(math.floor(g.x/32),math.floor(g.y/32))sleep(100)reconnect(Y,worldFirstStorageId,math.floor(g.x/32),math.floor(g.y/32))end while inventory:getItemCount(firstSeedId)<=0 do bot:collectObject(g.oid,10)sleep(100)reconnect(Y,worldFirstStorageId,math.floor(g.x/32),math.floor(g.y/32))end end if g.id==secondSeedId then while not bot:isInTile(math.floor(g.x/32),math.floor(g.y/32))and inventory:getItemCount(secondSeedId)<=0 do bot:findPath(math.floor(g.x/32),math.floor(g.y/32))sleep(100)reconnect(Y,worldFirstStorageId,math.floor(g.x/32),math.floor(g.y/32))end while inventory:getItemCount(secondSeedId)<=0 do bot:collectObject(g.oid,10)sleep(100)reconnect(Y,worldFirstStorageId,math.floor(g.x/32),math.floor(g.y/32))end end if inventory:getItemCount(firstSeedId)>0 and inventory:getItemCount(secondSeedId)>0 then break end end if inventory:getItemCount(firstSeedId)>0 and inventory:getItemCount(secondSeedId)>0 then break end elseif Y==getLastFirst and f+"\119\069"then hookInfo("\079\117\116 \111\102 \115\116\111\099\107 \105\110 \111\110\101 \111\102 \116\104\101 \115\101\101\100\115\044 \071\111\105\110\103 \116\111 \115\116\111\112 \066\111\116")sleep(100)lastDrop()sleep(100)bot:leaveWorld()sleep(100)bot:stopScr
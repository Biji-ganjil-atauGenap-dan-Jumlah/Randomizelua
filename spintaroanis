..("\042\042"..(loopz.." \108\111\111\112\042\042"))end end function timeLapse()splcIngfo=""if censorWorld then for I,A in pairs(splicer)do local R=string.upper(A)if R then local I=string.sub(R,1,censorWord)if I then splcIngfo=splcIngfo..("\n"..(I..("\124"..((waktu[R]or"\063")..("\124"..((statusWorld[R]or"\063")..("\124"..(foss[R]or"\048"))))))))end end end else for I,A in pairs(splicer)do splcIngfo=splcIngfo..("\n"..(A:upper()..("\124"..((waktu[A]or"\063")..("\124"..((statusWorld[A]or"\063")..("\124"..(foss[A]or"\048"))))))))end end end function taking()hookInfo("\084\097\107\105\110\103")sleep(100)if modeStorage=="\115\097\109\101"then for I,A in pairs(str1)do warping(A,worldFirstStorageId)sleep(100)local R=scanFloatFirst()<=0 or scanFloatSecond()<=0 if scanFloatFirst()>0 and scanFloatSecond()>0 then for I,R in pairs(world:getObjects())do if R.id==firstSeedId then while not bot:isInTile(math.floor(R.x/32),math.floor(R.y/32))and inventory:getItemCount(firstSeedId)<=0 do bot:findPath(math.floor(R.x/32),math.floor(R.y/32))sleep(100)reconnect(A,worldFirstStorageId,math.floor(R.x/32),math.floor(R.y/32))end while inventory:getItemCount(firstSeedId)<=0 do bot:collectObject(R.oid,10)sleep(100)reconnect(A,worldFirstStorageId,math.floor(R.x/32),math.floor(R.y/32))end end if R.id==secondSeedId then while not bot:isInTile(math.floor(R.x/32),math.floor(R.y/32))and inventory:getItemCount(secondSeedId)<=0 do bot:findPath(math.floor(R.x/32),math.floor(R.y/32))sleep(100)reconnect(A,worldFirstStorageId,math.floor(R.x/32),math.floor(R.y/32))end while inventory:getItemCount(secondSeedId)<=0 do bot:collectObject(R.oid,10)sleep(100)reconnect(A,worldFirstStorageId,math.floor(R.x/32),math.floor(R.y/32))end end if inventory:getItemCount(firstSeedId)>0 and inventory:getItemCount(secondSeedId)>0 then break end end if inventory:getItemCount(firstSeedId)>0 and inventory:getItemCount(secondSeedId)>0 then break end elseif A==getLastFirst and R then hookInfo("\079\117\116 \111\102 \115\116\111\0